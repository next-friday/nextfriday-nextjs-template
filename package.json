{
  "name": "create-nextfriday-nextjs",
  "version": "0.0.1",
  "private": true,
  "description": "Next Friday NextJS template with TypeScript, ESLint, Prettier, and Tailwind CSS",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbopack",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.mjs --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "sort-package-json": "pnpm exec sort-package-json",
    "sort-package-json:check": "pnpm exec sort-package-json --check",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@next/eslint-plugin-next": "^15.3.4",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-airbnb-extended": "^2.1.1",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.20.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sort-package-json": "^3.3.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "typescript-eslint": "^8.35.1"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
